{
  "name": "typeorm-sample",
  "version": "0.0.1",
  "description": "Job Board project developed with TypeORM.",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.17.21",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.14.1",
    "pg-error-enum": "^1.0.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.24",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "nodemon": "nodemon src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --dataSource src/data-source.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource src/data-source.ts"
  }
}
